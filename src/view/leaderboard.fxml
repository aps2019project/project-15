<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.leaderboard">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/scenes/shimzar/bg.jpg" />
         </image>
      </ImageView>
      <TextArea fx:id="leaderBoard" editable="false" layoutX="34.0" layoutY="14.0" opacity="0.5" prefHeight="806.0" prefWidth="978.0" text="LeaderBoard">
         <font>
            <Font size="27.0" />
         </font></TextArea>
      <Button blendMode="DARKEN" layoutX="847.0" layoutY="893.0" mnemonicParsing="false" onMouseClicked="#exit" prefHeight="71.0" prefWidth="165.0" text="Return" textAlignment="CENTER" textFill="#0a121b">
         <font>
            <Font name="Serif Regular" size="28.0" />
         </font>
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
   </children>
</Pane>
